# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key

# Server Configuration
PORT=5000
HOST=0.0.0.0
DEBUG=False
LOG_LEVEL=INFO

# API Configuration
# Set to "none" to disable API key validation
API_KEY=bedrock

# Agent Configuration
DEFAULT_AGENT=COBWEBAI-ALIAS

# Agent COBWEBAI-ALIAS
AGENT_COBWEBAI_ALIAS_ID=your-agent-id
AGENT_COBWEBAI_ALIAS_ALIAS_ID=your-alias-id

# Add more agents by following this pattern:
# AGENT_ANOTHER_AGENT_ID=your-agent-id
# AGENT_ANOTHER_AGENT_ALIAS_ID=your-alias-id